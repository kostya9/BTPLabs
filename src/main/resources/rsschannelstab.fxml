<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<AnchorPane minHeight="330.0" minWidth="600.0" style="-fx-padding: 10px 0 0 0"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ksharovarsky.lab3.ui.RssChannelsTabController">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" spacing="15">
            <children>
                <HBox AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" spacing="10" style="-fx-padding: 0 0 0 10px;">
                 <children>
                      <Label text="Name">
                          <font>
                              <Font size="16.0" />
                          </font>
                      </Label>
                     <TextField fx:id="nameField" prefHeight="25.0" prefWidth="147.0"/>
                      <Label text="Url">
                          <font>
                              <Font size="16.0" />
                          </font>
                      </Label>
                     <TextField fx:id="urlField" prefHeight="25.0" prefWidth="247.0" />
                     <Button fx:id="addButton" mnemonicParsing="false" text="Add Channel" />
                 </children>
                </HBox>
                <TableView fx:id="tableChannels" maxHeight="300.0" minWidth="500.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.bottomAnchor="0.0">
                  <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY" /></columnResizePolicy>
                  <columns>
                    <TableColumn fx:id="nameTableColumn" minWidth="100" text="Name" />
                    <TableColumn fx:id="urlTableColumn" minWidth="300" text="Url" />
                    <TableColumn fx:id="removeTableColumn" minWidth="100" sortable="false" text="" />
                </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
